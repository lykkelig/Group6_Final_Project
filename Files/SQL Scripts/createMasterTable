(
    "EDUCATIONFIELD_FK" integer,
    "EDUCATIONFIELD" text,
    PRIMARY KEY ("EDUCATIONFIELD_FK")
);

ALTER TABLE IF EXISTS public."CD_EDUCATIONFIELD"
    OWNER to postgres;

